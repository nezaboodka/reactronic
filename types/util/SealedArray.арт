  тип SealedArray<T> = объект на основе Array<T>, Sealed<Array<T>>
  {
    операция pop(): T?
    операция push(items: Массив<T>): Число
    операция sort(compareFn: операция(a: T, b: T): Число): this
    операция splice(start: Число, deleteCount: Число? = пусто): Массив<T>
    операция unshift(items: Массив<T>): Число
    // операция [Sealant.CreateCopy](): Array<T>
    операция slice(start: Число? = пусто, end: Число? = пусто): Массив<T>
  }
}
