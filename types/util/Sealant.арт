используется артель

внешние
{
  тип Sealable<T> = интерфейс
  {
    операция toMutable(): T
    // [Sealant.SealedType]: object
  }

  тип Sealed<T> = интерфейс
  {
    // [Sealant.CreateCopy]?: () => T
  }

  тип Sealant = объект
  {
    общее постоянное SealedType: UniqueSymbol
    общее постоянное CreateCopy: UniqueSymbol
    общая операция seal<T: Sealable<T>>(collection: T, sealedType: Object, typeName: Текст, member: Нечто): T
    общая операция toMutable<T: Sealable<T>>(collection: T): T
    общая операция error_(collection: Sealed<Нечто>): Error
  }
}
