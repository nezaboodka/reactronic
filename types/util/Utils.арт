используется артель

внешние
{
  тип F<T> = операция(args: Массив<Нечто>): T

  тип Utils = объект
  {
    общая операция freezeSet<T>(obj: Set<T>? = пусто): Set<T>?
    общая операция freezeMap<K, V>(obj: Map<K, V>? = пусто): Map<K, V>?
    общая операция copyAllMembers(source: Нечто, target: Нечто): Нечто
  }

  операция UNDEF(args: Массив<Нечто>): never
  операция all(promises: Array<Promise<Нечто>>): Promise<Массив<Нечто>>
  операция pause<T>(timeout: Число): Promise<T>
}
