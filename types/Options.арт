используется артель

внешние
{
 тип SnapshotOptions = интерфейс
 {
    постоянное hint: Текст?
    постоянное separation: SeparationMode?
    постоянное journal: Journal?
    постоянное logging: Partial<LoggingOptions>?
    постоянное token: Нечто?
  }

 тип MemberOptions = интерфейс
 {
    постоянное kind: Kind
    постоянное separation: SeparationMode
    постоянное order: Число
    постоянное noSideEffects: Булево
    постоянное triggeringArgs: Булево
    постоянное throttling: Число
    постоянное reentrance: Reentrance
    постоянное journal: Journal?
    постоянное monitor: Monitor?
    постоянное logging: Partial<LoggingOptions>?
  }

  тип Kind = вариант
  {
    Plain = 0
    Transactional = 1
    Reactive = 2
    Cached = 3
  }

  тип Reentrance = вариант
  {
    PreventWithError = 1
    WaitAndRestart = 0
    CancelPrevious = -1
    CancelAndWaitPrevious = -2
    OverwritePrevious = -3
    RunSideBySide = -4
  }
}
